{
	"manifest_version": 2,
	"name": "Minimal cookies",
	"version": "1.1",
	"icons": {
		"128": "logo-bg-hd128.png"
	},
	"permissions": [
		"storage",
		"cookies",
		"<all_urls>"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "logo-bg128.png",
		"default_popup": "popup.html",
		"default_title": "Minimal cookies"
	}
}